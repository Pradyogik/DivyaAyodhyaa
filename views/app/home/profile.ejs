                    </a>
                    <a href="/sendfeedback">
                        <div class="profilediv" style="height:55px;">

                            <div class="">
                                <img src="/app-assets/img/services/Group 1000004739 (4).png" style="height: 30px;">
                            </div>
                            <div class="profiledivhead">
                                <h4 style="margin-top: 6px; font-size: 17px;"> Send Feedback</h4>
                                <i class="fa fa-angle-right "
                                    style=" margin-top: -22px; color: #191914; font-size: 25px;"></i>
                            </div>

                        </div>
                    </a>

                    <a href="/logout">
                        <div class="profilediv" style="height:55px;">

                            <div class="">
                                <img src="/app-assets/img/services/Group 1000004739 (5).png" style="height: 30px;">
                            </div>
                            <div class="profiledivhead">
                                <h4 style="margin-top: 6px; font-size: 17px;"> Log Out</h4>

                            </div>

                        </div>
                    </a>





                </div>
            </section>
        </main>


        <%- include('../layouts/footer') %>
    </body>


</html>